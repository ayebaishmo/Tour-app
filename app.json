{
  "expo": {
    "name": "TourApp",
    "slug": "Tour_app",
    "version": "1.0.0",
    "scheme": "myapp",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#003399"
    },
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tour.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.tour.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiKey": "process.env.TOUR_APP_API_KEY",
      "messagingId": "process.env.TOUR_APP_MESSAGING_SENDER_ID",
      "appId": "process.env.TOUR_APP_APP_ID",
      "measurementId": "process.env.TOUR_APP_MEASUREMENT_ID",
      "googleMaps": {
        "apiKey": "AIzaSyCYeFTZcQ0ptQrdS9v0BKkH3tQrV_pQ47w"
      },
      "baseUrl": "https://maps.googleapis.com/maps/api/place"
    }
  }
}
